{
  "name": "desafio-loomi",
  "version": "1.0.0",
  "description": "Desafio t√©cnico loomi",
  "main": "index.js",
  "scripts": {
    "test:iphone": "cypress run --config viewportWidth=390,viewportHeight=844",
    "test:android": "cypress run --config viewportWidth=2340,viewportHeight=1080",
    "test:chrome": "cypress run --browser chrome",
    "test:firefox": "cypress run --browser firefox",
    "test:edge": "cypress run --browser edge",
    "test": "cypress run --config video=false --env allure=true,email=$EMAIL,senha=$PASSWORD --browser chrome --headless || true",
    "test:run": "cypress run --config video=false --env allure=true --browser chrome --headless || true",
    "allure:report": "allure generate allure-results",
    "allure:clear": "rm -r allure-results/ allure-report || true",
    "test:cy": "npm run allure:clear && npm run test && npm run allure:report"
  },
  "author": "Marillia Dantas",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@shelex/cypress-allure-plugin": "^2.40.2",
    "cypress": "^12.2.0"
  }
}